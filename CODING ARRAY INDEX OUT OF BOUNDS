package selfProgramming;

import java.util.*;

class ArrayIndexOutOfBounds {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double[] list = new double[10];
		int i;
		
		for (i = 0; i <= 10; i++) {
			list[i] = 5;
			System.out.println("Amount are : " + "list " + i + " = " + list[i]);
		}
	}

}
